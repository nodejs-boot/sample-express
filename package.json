{
    "name": "@nodeboot/express-sample",
    "version": "2.2.4",
    "description": "Node-Boot sample project using Express",
    "author": "Manuel Santos <ney.br.santos@gmail.com>",
    "license": "MIT",
    "private": "true",
    "keywords": [
        "node-boot",
        "express"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejs-boot/sample-express.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "src/index.ts",
    "scripts": {
        "start": "pnpm run clean:build && pnpm run build && node dist/server.js",
        "start:prod": "pnpm run build && NODE_ENV=production node dist/server.js",
        "dev": "NODE_ENV=development nodemon",
        "nodeboot:update": "pnpm update @nodeboot/*@latest",
        "build": "tsc -p tsconfig.build.json",
        "postbuild": "npx @nodeboot/aot node-boot-aot",
        "clean:build": "rimraf ./dist",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "pnpm lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "jest --passWithNoTests",
        "rebuild:sqlite": "npm rebuild better-sqlite3",
        "typecheck": "tsc",
        "create:migration": "typeorm migration:create ./src/persistence/migrations/migration"
    },
    "dependencies": {
        "@nodeboot/config": "^1.3.1",
        "@nodeboot/context": "^1.13.0",
        "@nodeboot/core": "^1.15.0",
        "@nodeboot/starter-openapi": "^2.4.2",
        "@nodeboot/express-server": "^1.7.0",
        "@nodeboot/authorization": "^1.3.1",
        "@nodeboot/di": "^1.4.1",
        "@nodeboot/error": "^1.3.0",
        "@nodeboot/starter-actuator": "^1.7.1",
        "@nodeboot/starter-persistence": "^1.11.0",
        "@nodeboot/starter-scheduler": "^1.6.0",
        "@nodeboot/starter-http": "^3.5.1",
        "@nodeboot/starter-validation": "^1.2.1",
        "@nodeboot/aot": "^1.4.0",
        "katxupa": "^1.10.4",
        "reflect-metadata": "^0.2.1",
        "typeorm": "^0.3.27",
        "express": "^4.21.2",
        "body-parser": "^1.20.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "better-sqlite3": "^12.4.1",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "hpp": "^0.2.3",
        "minimist": "^1.2.8",
        "multer": "^2.0.2",
        "cookie": "0.7.0",
        "express-session": "^1.17.3",
        "typedi": "^0.10.0",
        "luxon": "^3.0.0",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/hpp": "^0.2.2",
        "@types/multer": "^2.0.0",
        "@types/supertest": "^2.0.12",
        "@types/luxon": "^3.0.0",
        "@changesets/cli": "^2.28.1",
        "@jest/globals": "^29.5.0",
        "@swc/core": "^1.3.41",
        "@swc/jest": "^0.2.24",
        "@tsconfig/node-lts-strictest": "^18.12.1",
        "@types/jest": "^29.5.11",
        "@types/memwatch-next": "^0.3.6",
        "@types/node": "^22.14.1",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.31.0",
        "eslint-config-katxupa": "^1.0.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^4.4.3",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^13.1.2",
        "memwatch-next": "^0.3.0",
        "nodemon": "^2.0.21",
        "pinst": "^3.0.0",
        "prettier": "^2.8.3",
        "prettier-plugin-organize-imports": "^3.2.4",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    },
    "packageManager": "pnpm@10.4.1"
}
